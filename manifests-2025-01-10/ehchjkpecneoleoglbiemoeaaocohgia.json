        "_execute_action": {
            "suggested_key": {
                "windows": "Ctrl+Shift+E",
                "mac": "Command+Shift+E",
                "chromeos": "Ctrl+Shift+E",
                "linux": "Ctrl+Shift+E"
            },
            "description" : "Start the extension"
        },
        "go-to-the-path": {
            "suggested_key": {
                "windows": "Ctrl+E",
                "mac": "Command+E",
                "chromeos": "Ctrl+E",
                "linux": "Ctrl+E"
            },
            "description" : "Go to the path"
        },
        "new-tab": {
            "suggested_key": {
                "windows": "Ctrl+Shift+X",
                "mac": "Command+Shift+X",
                "chromeos": "Ctrl+Shift+X",
                "linux": "Ctrl+Shift+X"
            },
            "description" : "Open in new tab"
        }
