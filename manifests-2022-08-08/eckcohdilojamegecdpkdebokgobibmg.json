  "icons": { "16": "icons/icon16.png",
   "48": "icons/icon48.png",
   "128": "icons/icon128.png" 
 },
 "web_accessible_resources" : [
   {
     "resources":[
       "/views/custom-description.html",
       "icons/icon30.png",
       "icons/login.png",
       "icons/order.png",
       "libs/editor/ui/icons.svg",
       "icons/tooltip.png",
