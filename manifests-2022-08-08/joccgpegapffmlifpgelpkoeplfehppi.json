{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "ImGrab",
  "description": "This extension grabs images from Facebook Comments",
  "author": "Logan Martel",
  "version": "1.1.0",

  "icons": { 
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png" 
  },

  "browser_action": {
    "default_icon": "img/icon40.png",
    "default_popup": "html/popup.html"
  },

  "background": {
  	"page": "html/popup.html"
  },

  "permissions": [
  "tabs",
  "storage",
  "<all_urls>"
  ]
}
