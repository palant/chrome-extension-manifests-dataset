        "js": [
            "public/foreground.js"
        ],
        "all_frames": false
    }],
    "browser_action": {
        "default_title": "Beanote",
        "default_icon": "public/icons/icon-chrome.png",
        "default_popup": "public/actionPage/index.html"
    },
    "web_accessible_resources": [
        "public/*",
        "public/images/*"
    ],
