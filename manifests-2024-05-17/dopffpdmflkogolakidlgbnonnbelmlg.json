    "killAudibleTabs": {
      "suggested_key": {
        "default": "Ctrl+Shift+Comma",
        "mac": "Command+Shift+Comma"
      },
      "description": "Kills the muted tabs."
    }
  },

  "permissions": [
    "tabs"
  ],

  "offline_enabled": true
