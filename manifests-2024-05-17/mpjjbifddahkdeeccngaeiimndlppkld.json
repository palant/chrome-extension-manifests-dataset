{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "mp-block",
    "description": "block users in MP",
    "version": "0.0.1",

    "browser_action": {
        "default_icon": {
            "16": "icon-16.png",
