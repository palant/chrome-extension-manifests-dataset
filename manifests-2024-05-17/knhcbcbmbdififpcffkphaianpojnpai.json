  "page": "background.html"
 },
 "options_page": "options.html",
 "browser_action": 
 {
  "default_icon": "images/icon-plauser-128.png"
 },
 "content_scripts": 
 [
     {
       "matches": ["http://*/*", "https://*/*"],
       "js": ["js/content-scripts/content-script-key-event.js", "js/global.js"],
