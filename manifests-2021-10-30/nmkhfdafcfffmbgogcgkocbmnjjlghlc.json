{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,

	"name": "Page Translator",
	"description": "Übersetzt komplette Internetseiten mit Hilfe von Google Translate.",
	"version": "0.2",

	"icons": {
		"16": "data/img/icon16.png",
		"32": "data/img/icon32.png",
		"48": "data/img/icon48.png",
		"64": "data/img/icon64.png",
		"96": "data/img/icon96.png",
		"128": "data/img/icon128.png"
	},

	"background": {
		"persistent": false,
		"scripts": [ "background.js" ]
	},

	"options_page": "options.html",

	"browser_action": {
		"default_title": "Seite übersetzen",
		"default_icon": "data/img/icon.png"
	},

	"permissions": [ 
		"storage",
		"activeTab",
		"tabs",
		"notifications"
	]
}
