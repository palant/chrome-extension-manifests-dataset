{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Voxulus",
    "description": "Accessible Web for Everyone",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "/scripts/background.js"
    },
    "content_scripts": [
