{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Imgur Extender",
  "short_name": "iX",
  "description": "Hopefully people will stop bothering @Sarah for new loading gifs every 30 seconds",
  "version": "1.9",
  "icons": { 
   "16": "icon/16.png",
