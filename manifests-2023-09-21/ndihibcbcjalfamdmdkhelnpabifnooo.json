                "js/vendor.js",
                "js/presence.js"
            ],
            "run_at": "document_start"
        }
    ],
    "background": {
        "service_worker": "js/background.js"
    },
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "permissions": [
        "management",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
