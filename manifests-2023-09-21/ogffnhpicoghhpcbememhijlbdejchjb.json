                "js/pages/discover.js",
                "js/pages/games.js",
                "js/pages/groups.js",
                "js/pages/home.js",
                "js/pages/profile.js",
                "js/pages/settings.js",
                "js/pages/universal.js",

                "js/util.js"
            ],

            "run_at": "document_start",
            "all_frames": true,
            "matches": ["*://*.roblox.com/*"]
        }
