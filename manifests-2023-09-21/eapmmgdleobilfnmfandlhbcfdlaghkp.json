{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "TubeShift",
    "version": "0.0.20.0",
    "description": "Find the same video on other video websites",
    "icons": {
      "48": "icons/tubeshift-48.png",
      "96": "icons/tubeshift-96.png"
    },

    "permissions": [
      "storage",
      "tabs",
      "webNavigation"
    ],

    "optional_permissions": [
      "https://www.bitchute.com/*",
      "https://www.odysee.com/*",
      "https://api.lbry.com/*",
      "https://www.youtube.com/*"
    ],

    "options_ui": {
      "page": "options.html"
    },

    "background": {
      "scripts": [
        "js/tubeshift-api-experiment.js",
        "js/tubeshift-module.js",
        "js/tubeshift-browser.js",
        "platform/bitchute.js",
        "platform/dailymotion.js",
        "platform/odysee.js",
        "platform/rumble.js",
        "platform/youtube.js",
        "background.js"
      ]
    },

    "browser_action": {
      "browser_style": true,
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/buttonbar.inactive-16.png",
        "32": "icons/buttonbar.inactive-32.png"
      }
    },

    "web_accessible_resources": [
      "/icons/tubeshift-overlay.svg"
    ]
}
