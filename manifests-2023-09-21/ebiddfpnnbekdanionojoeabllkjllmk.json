  },
  "icons":{
    "16":"icon16.png",
    "48":"icon48.png",
    "128":"icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://vipsearch.top/*"],
      "js": ["content.js"]
    }
  ],
  "background":{
   "scripts":[
     "background.js"
   ]
 },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
