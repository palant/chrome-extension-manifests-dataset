{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "key": "",
  "name": "Let's Recast Extension",
  "version": "1.3.0.10",
  "description": "Browser Extension for letsrecast.ai",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_specific_settings": {
    "safari": {
      "strict_min_version": "16.4"
