    "manifest_version": 3,
    "permissions": [
      "activeTab",
      "storage",
      "scripting"
    ],
    "host_permissions": [
      "https://*.openai.com/*",
      "https://promptperfect.jina.ai/*",
      "https://nocodeplugins.tech/*"
    ],
    "content_scripts": [
        {
          "matches": ["https://chat.openai.com/*"],
          "js": ["content.js"]
        }
      ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "My PromptPerfect Extension",
      "default_icon": {
        "16": "icon16.png",
        "32": "icon32.png"
      }
    },
    "options_page": "options.html",
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  



