{
"update_url": "https://clients2.google.com/service/update2/crx",


  "manifest_version": 2,

  "name": "QuickShift Redux - Rearrange Chrome Tabs",
  "short_name": "QuickShift Redux",
  "description": "Easily and quickly rearrange tabs with keyboard shortcuts.",
  "version": "1.2.0",
  "author": "Kenneth Lu",

  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },

  "commands": {
    "move-tabs-left": {
      "description": "Move highlighted tabs to the left",
      "suggested_key": {
        "default": "Ctrl+Shift+Left",
        "mac": "MacCtrl+Shift+Left"
      }
    },

    "move-tabs-right": {
      "description": "Move highlighted tabs to the right",
      "suggested_key": {
        "default": "Ctrl+Shift+Right",
        "mac": "MacCtrl+Shift+Right"
      }
    },

    "undock-tabs-to-new-window": {
      "description": "Undock highlighted tabs to a new window",
      "suggested_key": {
        "default": "Ctrl+Shift+U",
        "mac": "MacCtrl+Shift+U"
      }
    },

    "move-tabs-between-windows": {
      "description": "Move tabs between windows",
      "suggested_key": {
        "default": "Ctrl+Shift+M",
        "mac": "MacCtrl+Shift+M"
      }
    }
  }

}
