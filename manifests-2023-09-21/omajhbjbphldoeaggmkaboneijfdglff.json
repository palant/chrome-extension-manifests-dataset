{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Web Travel",
  "version": "0.5.3",
  "manifest_version": 2,
  "description": "It is a fun extension which tells and shows you how much your browser travlled for any page you visited.",
  "icons": {
    "16": "icons/16.png",
    "28": "icons/28.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "background": {
    "page": "src/bg/bg.html"
  },
   "content_scripts": [
    {
      "matches": ["https://*/* ","http://*/* "],
      "js": ["src/options_custom/js/script.js"],
      "run_at": "document_end"
    }
  ],
  "page_action": {
    "default_title": "Web Travel",
    "default_popup": "src/options_custom/map.html",
     "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "options_page": "src/options_custom/index.html",
  "permissions": [
    "geolocation",
    "tabs",
    "storage",
    "webNavigation",
    "webRequest",
    "https://*/* ",
    "http://*/* "
  ],
  "content_security_policy": "script-src 'self' https://maps.google.com https://google.com; object-src 'self'"
}