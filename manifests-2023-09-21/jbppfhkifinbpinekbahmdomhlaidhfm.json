      "matches": ["file://*/*", "http://*/*", "https://*/*"],
      "js": ["app/content-script.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["app/inpage.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": {}
