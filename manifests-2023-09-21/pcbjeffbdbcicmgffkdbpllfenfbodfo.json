                  "https://www.google.com.gr/*",
                  "https://www.google.co.in/*",
                  "https://www.google.com.mx/*",
                  "https://www.google.dk/*",
                  "https://www.google.com.ar/*",
                  "https://www.google.ch/*",
                  "https://www.google.cl/*",
                  "https://www.google.at/*",
                  "https://www.google.co.kr/*",
                  "https://www.google.ie/*",
                  "https://www.google.com.co/*",
                  "https://www.google.pl/*",
                  "https://www.google.pt/*",
                  "https://www.google.com.pk/*"
                ],
      "js": ["searchFilter.js"],
      "run_at": "document_end"
