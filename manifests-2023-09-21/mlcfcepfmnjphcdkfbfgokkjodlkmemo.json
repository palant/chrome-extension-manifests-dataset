"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Site Bleacher",
  "description": "Remove automatically cookies, local storages, IndexedDBs, service workers, cache storages, filesystems and webSQLs",
  "version": "1.2.1",
  "permissions": [
    "storage",
    "tabs",
    "cookies",
    "history",
    "browsingData",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "common.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2
}
