      "matchAboutBlank":true,
      "all_frames":true,
      "js": ["content"]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "matchAboutBlank":true,
      "all_frames":true,
      "js": ["js/jquery.min.v1.12.4.js"]
    }
  ],
  "icons": { 
    "16": "img/boozang16.png",
    "48": "img/boozang48.png",
    "128": "img/boozang128.png"
  },
  "browser_action": {
    "default_icon": "img/boozang.png",
    "default_title": "Launch Boozang",
    "default_popup": "popup.html"
