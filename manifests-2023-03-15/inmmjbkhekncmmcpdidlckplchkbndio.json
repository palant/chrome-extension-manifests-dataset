{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "NRC Unlimited",
  "description": "Deze extensie blokkeert de betaalmuur van NRC.nl waardoor je ongelimiteerd artikelen kunt lezen op de website.",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
