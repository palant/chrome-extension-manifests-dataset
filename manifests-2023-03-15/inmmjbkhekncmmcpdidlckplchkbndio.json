      "32": "images/icon32.png",
      "64": "images/icon64.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "24": "images/icon24.png",
    "32": "images/icon32.png",
    "64": "images/icon64.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "cookies",
    "*://*.nrc.nl/"
  ],
  "background": {
    "scripts": ["background_scripts/cookies.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.nrc.nl/nieuws/*"],
      "js": ["content_scripts/inject.js"]
    }
  ]
