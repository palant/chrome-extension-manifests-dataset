      "https://www.amazon.com/*",
      "https://www.amazon.cn/*",
      "https://www.amazon.in/*",
      "https://www.amazon.co.jp/*",
      "https://www.amazon.sg/*",
      "https://www.amazon.com.tr/*",
      "https://www.amazon.ae/*",
      "https://www.amazon.fr/*",
      "https://www.amazon.de/*",
      "https://www.amazon.it/*",
      "https://www.amazon.nl/*",
      "https://www.amazon.es/*",
      "https://www.amazon.co.uk/*",
      "https://www.amazon.com.au/*"
