{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "DjVu.js Viewer",
    "short_name": "DV",
    "version": "0.9.0.0",
    "author": "RussCoder",
    "homepage_url": "https://github.com/RussCoder/djvujs",
    "description": "Opens links to .djvu files. Allows opening files from a local disk. Processes <object> & <embed> tags.",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_security_policy": "script-src 'self'; object-src 'self';",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "all_frames": true,
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "contextMenus"
    ],
    "web_accessible_resources": [
        "viewer.html"
    ],
    "icons": {
        "16": "djvu16.png",
        "32": "djvu32.png",
        "48": "djvu48.png",
        "64": "djvu64.png",
        "96": "djvu96.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "djvu16.png",
            "32": "djvu32.png",
            "48": "djvu48.png",
            "64": "djvu64.png",
            "96": "djvu96.png"
        }
    }
}