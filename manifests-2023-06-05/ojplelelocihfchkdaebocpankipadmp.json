  },

  "browser_action": {
    "default_icon": "images/ScopusIcon-32.png",
    "default_title": "Scopus Document Download Manager",
    "default_popup": "popup.html"
  },

  "content_scripts": [{
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "js": [
      "js/contentScript.js"
    ],
    "run_at": "document_start"
  }],

  "background": {
    "scripts": [
      "lib/pdf-locator-1.0.2.min.js",
      "js/backgroundScript.js"
