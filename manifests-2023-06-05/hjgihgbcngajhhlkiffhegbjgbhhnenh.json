                "js/scripts/knockout.custom.js",
                "js/scripts/moment.min.js",
                "js/yodaUtils.js",
                "js/yodaEntities.js",
                "js/yodaCache.js",
                "js/yodaSearch.js",
                "js/initExtension.js"
            ]
        }
    ]
