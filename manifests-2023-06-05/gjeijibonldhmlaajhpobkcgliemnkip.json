"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,

	"name": "Kill 9gag Feed",
	"description": "This extension removes the 9gag feed. Enable this while at work.",

	"version": "0.3",

	"permissions": [
		"http://9gag.com/"
	],

	"content_scripts": [
		{
			"matches": ["http://9gag.com/*"],
			"js": ["pages/script.js"]	
		}
	],

	"icons": {
		"16": "icons/icon-16.png",
		"32": "icons/icon-32.png",
		"64": "icons/icon-64.png",
		"128": "icons/icon-128.png"
	}
