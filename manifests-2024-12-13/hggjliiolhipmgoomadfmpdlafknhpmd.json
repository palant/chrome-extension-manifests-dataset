        "default_icon": {
            "16": "/icons/16.png",
            "48": "/icons/48.png",
            "128": "/icons/128.png"
        }
    },
    "content_scripts": [
        {
            "js": [
                "content.js",
                "puzzle.js"
            ],
            "matches": [
                "https://lichess.org/*"
            ]
        },
        {
            "js": [
                "quotes.js"
            ],
            "matches": [
                "https://lichess.org/"
            ]
