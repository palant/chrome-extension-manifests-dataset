{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Map TikTok locations from your saved videos",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "downloads"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.tiktok.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logopng.png",
      "48": "images/logopng.png",
      "128": "images/logopng.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["images/loading.gif"],
      "matches": ["<all_urls>"]
    }
  ]
}
