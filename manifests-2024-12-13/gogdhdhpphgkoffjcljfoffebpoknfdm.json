  "windows": "Shift+Alt+W" 
   },
      "description": "Next song"
    },
    "remote-toggle": {
      "suggested_key": { 
   "default": "Shift+Alt+E",
  "windows": "Shift+Alt+E" 
   },
      "description": "Toggle play/pause"
    }
  },
  "background": {
    "scripts": ["jquery-1.10.2.min.js", "remote.js"],
 "persistent": false
