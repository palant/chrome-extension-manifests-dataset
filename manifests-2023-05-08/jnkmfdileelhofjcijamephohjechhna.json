{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Google Analytics Debugger",
  "description": "Prints useful information to the JavaScript console by enabling the debug version of the Google Analytics Javascript.",
  "permissions": ["http://*/*", "https://*/*", "webRequest", "webRequestBlocking", "tabs"],
  "version": "2.8",

  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },

  "browser_action": {
    "default_title": "GA Debug: OFF",
    "default_icon": {
      "16": "icon-off-16.png",
      "32": "icon-off-32.png"
    }
  },

  "background": {
    "scripts": ["background.js"]
  }
}

