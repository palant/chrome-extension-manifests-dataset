  "browser_action": {
    "default_icon": "dist/img/icon.png"
  },
  "background": {
    "scripts": [
      "dist/assets/js/compat.js",
      "dist/assets/voicedata/tts/webtts.js",
      "dist/js/script.bundle.js"
