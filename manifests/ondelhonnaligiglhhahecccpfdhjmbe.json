{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,
	
	"name": 	"Paraguay.com Widget",
	
	"version": 	"1.0",
	
	"icons": 	{ 	
					"48": "icon48.png",
					"128": "icon128.png" 
	},
				
	"description": 	"Widget Paraguay.com - Noticias de Paraguay en tiempo real. Paraguay en Internet.",
	
	"browser_action": 	{
							"default_title": 	"Paraguay.com Widget",     
							"default_icon": 	"icon128.png",
							"default_popup": 	"popup.html"      
	},

	"permissions": 	[	
  						"http://www.paraguay.com/*",
  						"http://*.google.com/*"
	],
	
	"content_security_policy": 	"script-src 'self' https://ssl.google-analytics.com https://www.google.com; object-src 'self'"
}
