  "name": "Yizy",
  "description": "Yizy is a Unique Bookmark Management Platform to Manage Bookmarks, Screenshots, Images and Inspirations!",
  "version": "1.0.1",
  "web_accessible_resources": [
    "index.html"
  ],
  "browser_action": {
    "default_icon": "assets/icon.png"
