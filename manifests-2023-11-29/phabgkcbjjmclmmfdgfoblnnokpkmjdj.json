      {
          "matches": ["<all_urls>"],
          "js": ["ext/vendor.js", "ext/content-script.js"]
      }
  ],

  "background": {
    "scripts": ["ext/vendor.js", "ext/background.js"]
  },

  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
