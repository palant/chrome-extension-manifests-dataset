  "48": "images/icon48.png",
  "128": "images/icon128.png"
},
"content_scripts": [
  {
    "matches": ["<all_urls>"],
    "all_frames": true,
    "js": ["js/jquery-3.6.0.min.js","js/script.js"],
    "match_origin_as_fallback": true,
    "run_at": "document_start"
  }
]
