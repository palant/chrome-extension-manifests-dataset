    "action": {
        "default_icon": "/icons/icon-128.png",
        "default_title": "Chat-Gpt Broswer Plugin"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "exclude_globs": [
                "https://*.openai.com/*",
                "https://chatbot-testing.xuantong.cn/*"
            ],
            "js": [
                "/js/sidebar.js"
            ],
            "type": "module",
            "run_at": "document_start",
            "all_frames": false
        },
        {
            "matches": [
                "*://www.baidu.com/*"
            ],
            "js": [
                "/js/search.js"
            ],
            "type": "module",
            "run_at": "document_start",
            "all_frames": true
        },
        {
            "matches": [
                "*://chatbot-testing.xuantong.cn/*"
            ],
            "js": [
                "/js/cscu.js"
            ],
            "type": "module",
            "run_at": "document_start",
            "all_frames": true
        },
        {
            "matches": [
                "*://*.openai.com/*"
            ],
            "js": [
                "/js/csopenai.js"
            ],
            "type": "module",
            "run_at": "document_start",
            "all_frames": true
        }
    ],
    "options_page": "options.html",
    "permissions": [
        "storage",
        "tabs",
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "/*.html",
                "/js/*",
                "/css/*",
                "/icons/*"
            ],
            "matches": [
                "*://*/*"
            ]
        }
    ]
