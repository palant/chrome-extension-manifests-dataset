{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Chat Bot",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "Chat-Gpt Broswer Plugin",
    "background": {
        "service_worker": "/js/sw.js"
