{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name":"InsightsCRM LinkFinder",
    "version": "1.0.4",
    "manifest_version": 3,
    "description":"The extension enables users to effortlessly extract key information from profiles and seamlessly integrate it into InsightsCRM.",
    "icons": {
        "128": "./assest/images/favicon.png"
     },
    "background": {
      "service_worker": "background_worker.js",
      "type": "module"
   },
   "action": {
   "default_title": "InsightsCRM LinkFinder"
   },
   "content_scripts": [
        {
            "matches": ["https://*.linkedin.com/*"],
            "js": ["./libs/jquery.min.js","./foreground/content_invoker.js"],
            "all_frames" :false
        }
    ],
    "host_permissions": [
        "https://ext.insightscrm.com/*"
      ],
   "permissions": [
        "tabs",
        "storage",
        "activeTab"
    ],

    "web_accessible_resources": [
        {
            "resources": [ "frame/*","assest/*" ],
            "matches": ["https://*.linkedin.com/*"]
        }
    ],
    "homepage_url": "https://insightscrm.com/"
}