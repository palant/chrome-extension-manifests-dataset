        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens hello.html"
    }
  },
  "content_scripts": [
    {
       "matches": ["http://*/*", "https://*/*"],
       "run_at": "document_end",
       "js": ["Html/Toolbar.js","contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    "Html/Toolbar.html",
    "Html/Toolbar.css",
    "Html/ItemTemplate/WordListItemTemplate.html",
    "Html/Graph.html",
    "Html/Graph.js",
    "Popup.js"
