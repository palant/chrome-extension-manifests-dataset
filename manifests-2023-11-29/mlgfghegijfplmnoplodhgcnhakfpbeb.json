            }
        }
    },
   "content_scripts": [{
        "matches": ["<all_urls>"],
     "js": ["selection.js"]
    }],
    "icons": { "128" : "res/icon-128.png" },
    "permissions": [
        "http://*/",
        "https://*/",
        "activeTab", 
        "tabs", 
