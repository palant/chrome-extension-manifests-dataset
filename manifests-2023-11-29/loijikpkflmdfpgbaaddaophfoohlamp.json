 ],
 "content_scripts": [
  {
   "matches": ["https://www.youtube.com/*", "https://youtube.com/*"],
   "js": ["scripts/content.js"],
   "css": ["styles/styles.css"],
   "run_at": "document_idle"
  }
 ],
 "icons": {
  "16": "icons/icon16.png",
  "48": "icons/icon48.png",
  "128": "icons/icon128.png"
 },
 "web_accessible_resources": [
  {
   "resources": ["/icons/*"],
   "matches": ["https://www.youtube.com/*", "https://youtube.com/*"]
  }
 ]
