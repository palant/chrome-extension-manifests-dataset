{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "JobGenie",
    "description": "Chrome Extension that allows you to apply for jobs on LinkedIn",
    "version": "1.7.0",
    "background": {
        "service_worker": "/js/background.js"
    },
    "content_scripts": [
        {
            "all_frames": false,
            "js": [
                "/js/content.js"
            ],
            "matches": [
                "https://*.linkedin.com/*"
            ],
            "run_at": "document_end"
        }
    ],
    "action": {
        "default_icon": "/assets/images/jb48.png",
        "default_title": "JobGenie",
        "default_popup": "popup.html"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self' ; object-src 'self'"
    },
    "icons": {
        "16": "/assets/images/jb48.png",
        "32": "/assets/images/jb48.png",
        "64": "/assets/images/jb48.png",
        "128": "/assets/images/jb48.png"
    },
    "permissions": [
        "storage",
        "tabs",
        "notifications",
        "background"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "/assets/*"
            ],
            "matches": [
                "*://*.linkedin.com/*"
            ]
        }
    ],
    "host_permissions": [
        "*://*.linkedin.com/*",
        "*://jobgenie.herokuapp.com/*"
    ]
}
