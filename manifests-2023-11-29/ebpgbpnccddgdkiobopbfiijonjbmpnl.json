      "js": [
        "content/content.js"
      ],
      "css": [
        "static/font.css",
        "content/content.css"
      ]
    }
  ],
  "action": {
    "default_popup": "popup/index.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "static/*",
        "map/*",
        "lib/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
