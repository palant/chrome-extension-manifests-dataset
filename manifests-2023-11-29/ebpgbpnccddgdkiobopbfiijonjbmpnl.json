  "name": "SpyBuster",
  "description": "Unmask a spy. Save your data.",
  "version": "1.0",
  "manifest_version": 3,
  "minimum_chrome_version": "92",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "type": "module",
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
