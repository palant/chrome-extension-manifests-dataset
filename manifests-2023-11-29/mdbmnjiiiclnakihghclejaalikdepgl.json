  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "offline_enabled": true,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle",
      "all_frames": false
    }
