{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Personium Google Chrome Extension",
  "description": "This extension is used for catching recruiters on sites: HeadHunter, SuperJob, Rabota.ru, LinkedIn, RestoJob and etc.",
  "version": "0.2",
  "default_locale": "ru",

  "browser_action": {
    "default_icon": {
      "38": "images/icon38.png"
    },
    "default_title": "__MSG_defaultTitle__"
  },

  "author": "Personium, LLC",
  "background": {
    "persistent": true,
    "scripts": [
      "js/md5.min.js",
      "js/api.min.js",
      "js/background.min.js"
    ]
  },

  "content_scripts": [
    {
      "matches": [
        "*://*.hh.ru/resume/*",
        "*://hh.ru/resume/*"
      ],
      "css": [],
      "js": [
        "js/jquery.min.js",
        "js/parsers/hh.min.js",
        "js/content.min.js"
      ]
    },
    {
      "matches": [
        "*://*.linkedin.com/in/*",
        "*://linkedin.com/in/*"
      ],
      "css": [],
      "js": [
        "js/jquery.min.js",
        "js/parsers/linkedin.min.js",
        "js/content.min.js"
      ]
    },
    {
      "matches": [
        "*://*.restojob.ru/worker/*",
        "*://restojob.ru/worker/*"
      ],
      "css": [],
      "js": [
        "js/jquery.min.js",
        "js/parsers/restojob.min.js",
        "js/content.min.js"
      ]
    },
    {
      "matches": [
        "*://*.rabota.ru/*",
        "*://rabota.ru/*"
      ],
      "css": [],
      "js": [
        "js/jquery.min.js",
        "js/parsers/rabota.min.js",
        "js/content.min.js"
      ]
    },
    {
      "matches": [
        "*://*.superjob.ru/resume/*",
        "*://superjob.ru/resume/*"
      ],
      "css": [],
      "js": [
        "js/jquery.min.js",
        "js/parsers/superjob.min.js",
        "js/content.min.js"
      ]
    }
  ],

  "permissions": [
    "tabs",
    "https://app.personium.ru/"
  ]
}