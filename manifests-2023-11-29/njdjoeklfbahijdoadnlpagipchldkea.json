  },
  "action": {
    "default_icon": {
      "16": "logos/gu_16x16.png",
      "32": "logos/gu_32x32.png"
    },
    "default_title": "GaiaUpgrade"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "login_rewards",
        "enabled": false,
