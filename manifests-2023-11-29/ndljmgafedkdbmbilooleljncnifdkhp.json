{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "AnyMock",
  "version": "1.0.4",
  "manifest_version": 3,
  "description": "mock & management",
  "permissions": [
    "storage",
    "tabs",
    "clipboardRead",
    "clipboardWrite",
    "unlimitedStorage"
  ],
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "options_page": "options.html",
  "devtools_page": "devtools.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [{
    "resources": ["js/script.js"],
    "matches": ["<all_urls>"]
  }]
}