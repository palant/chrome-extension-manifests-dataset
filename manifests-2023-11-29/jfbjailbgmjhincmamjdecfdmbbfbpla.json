{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version":2,

  "name":"Smart Search",
  "short_name": "smartsearch",
  "description":"Your personalized smart search engine for smart users!",
  "version":"1.0.8",
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://smartsearch.top/*"
  ],
  "optional_permissions": [
    "<all_urls>",
    "cookies",
