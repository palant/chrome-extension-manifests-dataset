  "host_permissions": [
    "https://localhost:3000/*",
    "*://*.overloop.com/*",
    "*://*.linkedin.com/*",
    "*://*.lightning.force.com/*",
    "*://*.herokuapp.com/*"
  ],
  "background": {
    "service_worker": "lib/background/service-worker.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "/lib/client/dist/*",
        "/lib/dist/*",
        "/assets/*"
      ],
      "matches": [ "<all_urls>" ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "*://*.overloop.com/*",
      "*://*.linkedin.com/*",
      "*://*.lightning.force.com/*",
      "*://localhost/*"
    ]
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; img-src 'self' * blob: data:; object-src 'self'"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.linkedin.com/*"
      ],
      "exclude_matches": [
        "*://*.linkedin.com/sales/*"
      ],
      "css": [
        "assets/css/styles.css"
      ],
      "run_at": "document_start",
