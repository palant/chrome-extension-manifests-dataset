{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Youtube Assistant",
    "description": "Chat with any video on youtube",
    "version": "0.0.0.1",
    "icons": {
        "16": "/images/16.png",
        "32": "/images/32.png",
        "48": "/images/48.png",
        "128": "/images/128.png"
    },
    "permissions": [
        "declarativeContent",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
      },
    "content_scripts": [{
        "matches": [
            "*://www.youtube.com/*",
            "*://youtube.com/*"
        ],
        "js": ["injector.js"],
        "run_at": "document_idle"
    }],
    "web_accessible_resources": [{
        "resources": ["he.min.js"],
        "matches": [
            "*://www.youtube.com/*",
            "*://youtube.com/*"
        ]
    }],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/16.png",
            "32": "/images/32.png",
            "48": "/images/48.png",
            "128": "/images/128.png"
        }
    }

