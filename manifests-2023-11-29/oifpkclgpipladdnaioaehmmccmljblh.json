      "js": ["common.js","keyboard.js","content.js"],
      "all_frames": false
    }
  ], 
    
  "icons": { "128": "loupe128.png" },
  "content_security_policy": "script-src 'self'; object-src 'self';",
           
  "permissions": [         
       "storage", 
        "tabs", 
        "management",
        "https://*/*", 
       "http://*/*" 
  ]
  
