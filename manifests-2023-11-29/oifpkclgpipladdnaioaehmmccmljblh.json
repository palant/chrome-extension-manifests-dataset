"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version": 2,

 "name": "Zoomba",
 "short_name": "Zoomba",
 "description": "Customizable zoom increment.", 
 "version": "1.2.1",
 "author": "appdevsw@wp.pl",

  "browser_action": {
    "default_title": "Zoomba",
    "default_popup": "popup.html"
