{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Subtitles For Russian YouTube",
  "manifest_version": 3,
  "version": "0.0.5",
  "description": "Use this extension to add subtitles available to Russian YouTube movie or series",
  "icons": {
    "16": "images/1.png",
    "48": "images/2.png",
    "128": "images/3.png"
  },  
  "permissions": [
    "notifications"
  ],
  "host_permissions": [
    "https://*.slokerdev.com/*"
  ],
  "web_accessible_resources": [{
    "resources": [
      "images/*.*"
    ],
    "matches": ["<all_urls>"]
  }],
  "content_scripts": [{
    "matches": ["*://www.youtube.com/*"],
    "js": ["js/libs/parser.js", "js/content-script.js"]
  }],
  "background": {
    "service_worker": "background.js"
  }
}
