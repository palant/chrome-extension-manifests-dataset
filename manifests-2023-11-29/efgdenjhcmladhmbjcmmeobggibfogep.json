{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "ORIOKS tweaker",
    "version": "2.0",
    "description": "Модификация для ОРИОКСа. Правильное отображение баллов, интеграция расписания занятий и кастомизация!",
    "icons": {
        "128": "assets/logo.png"
    },
    "background": {
        "service_worker": "background.js"
