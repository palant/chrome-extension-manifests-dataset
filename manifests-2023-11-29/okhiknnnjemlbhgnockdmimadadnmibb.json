{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "isSPA",
  "description": "This extension helps detecting single page applications.",
  "version": "1.1",
  "icons": { "16": "img/icon16.png",
    "48": "img/icon48.png",
