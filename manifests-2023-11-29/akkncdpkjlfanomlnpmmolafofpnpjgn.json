"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version" : 2,
 "name" : "Video m3u8 Sniffer - Find HLS Streaming URLs",
 "description" : "Grabs m3u8 webRequests from visited web pages and provides options for openning them.",
 "version" : "1.1.0",
