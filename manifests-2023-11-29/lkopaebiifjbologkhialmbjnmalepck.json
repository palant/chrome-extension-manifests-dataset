{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "ChatGPT for Wechat",
  "description": "Automatically respond to Wechat messages via chatgpt",
  "version": "4.1.8",
  "manifest_version": 3,
  "icons": {
    "16": "assets/icons/logo.png",
    "32": "assets/icons/logo.png",
    "48": "assets/icons/logo.png",
    "128": "assets/icons/logo_128.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "https://*.openai.com/",
    "https://*.bing.com/*",
    "https://*.google.com/*",
    "https://wx.qq.com/*",
    "https://web.wechat.com/*",
    "https://wx2.qq.com/*",
    "https://wx8.qq.com/*",
    "https://*.weixin.qq.com/*"
  ],
  "optional_host_permissions": [
    "http://*/*",
    "https://*/*",
    "*://*/*"
  ],
  "background": {
    "service_worker": "js/background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icons/logo.png",
      "32": "assets/icons/logo.png",
      "48": "assets/icons/logo.png",
      "128": "assets/icons/logo_128.png"
    },
    "default_title": "ChatGPT for Wechat"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.wx.qq.com/*",
        "https://*.wx2.qq.com/*",
        "https://*.wx8.qq.com/*",
        "https://*.web.wechat.com/*",
        "https://*.weixin.qq.com/*"
      ],
      "js": [
        "js/cnts.bundle.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "js/ips.bundle.js"
      ],
      "matches": [
        "https://*.wx.qq.com/*",
        "https://*.wx2.qq.com/*",
        "https://*.wx8.qq.com/*",
        "https://*.web.wechat.com/*"
      ]
    },
    {
      "resources": [
        "js/fhs.bundle.js"
      ],
      "matches": [
        "https://*.weixin.qq.com/*"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}