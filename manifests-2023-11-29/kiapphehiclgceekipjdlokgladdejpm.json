{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Knowlocker Screen Capturing",
    "author": "",
    "version": "1.0.4",
    "manifest_version": 2,
    "minimum_chrome_version": "34",
    "description": "This WebRTC screen capturing extension can be used within any WebRTC library/application!",
    "homepage_url": "https://www.knowlocker.com/",
    "background": {
        "scripts": [
            "background-script.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "js": [
                "content-script.js"
            ],
            "all_frames": true,
            "run_at": "document_end",
            "matches": [
                "*://knowlocker.loc/*", 
                "*://*.knowlocker.loc/*", 
                "*://knowlocker.com/*", 
                "*://*.knowlocker.com/*"
            ]
        }
    ],
    "externally_connectable": {
        "matches": [
            "*://knowlocker.loc/*", 
            "*://*.knowlocker.loc/*", 
            "*://knowlocker.com/*", 
            "*://*.knowlocker.com/*"
        ]
    },
    "icons": {
        "16": "icon16.ico",
        "48": "icon48.png",
        "128": "icon128.png",
        "512": "icon512.png"
    },
    "permissions": [
        "desktopCapture", 
        "tabs", 
        "http://*/", 
        "https://*/"
    ],
    "web_accessible_resources": [
        "icon16.ico",
        "icon48.png",
        "icon128.png",
        "icon512.png"
    ]
}