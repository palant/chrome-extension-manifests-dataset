    "debugger",
    "storage",
    "tabs",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "content_scripts": [
        {
        "matches": [
            "http://*/*",
            "https://*/*"
            ],
        "exclude_globs": [
                   "*pinterest.com/*",
                   "*twitter.com/*",
                   "*facebook.com/*",
                   "*calendar.google.com/*",
                   "*mail.*"
                   ],
        "css": ["css/jquery-ui.css", "css/style.css", "lib/css/font-awesome.min.css"],
        "js": ["js/jquery.min.js", "js/jquery-ui.min.js", "js/config.js", "js/profanity_words.js", "js/content.js", "js/script.js"],
        "run_at": "document_end"
        }
