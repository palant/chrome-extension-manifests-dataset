    "name": "Lodestone inventory grabber",
    "version": "1.2.5",
    "manifest_version": 2,
    "description": "A chrome extension to grab retainer's inventory from lodestone into your clipboard",
    "homepage_url": "https://ffxivteamcraft.com/recipe-finder",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
