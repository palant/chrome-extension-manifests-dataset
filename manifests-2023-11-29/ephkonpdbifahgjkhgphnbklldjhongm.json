        "popup.js",
        "popup.css",
        "img/invite_use_guide.png",
        "img/message_use_guide.png",
        "img/scrape_use_guide.png",
        "imgs/logo.png",
        "imgs/pinned.png",
        "imgs/guide.png"
      ],
