{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "SystemM Viewer",
  "version": "0.0.1.0",
  "description": "Simple markdown viewer to make it is easier and faster to read paragraphs and chapters.",
  "manifest_version": 3,
  "icons": {
    "16": "./images/c16.png",
    "32": "./images/c32.png",
    "128": "./images/c128.png"
  },
  "action": {
    "default_popup": "./index.html",
    "default_icons": {
      "16": "./images/c16.png",
      "32": "./images/c32.png",
      "128": "./images/c128.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*.systemm.co/*"],
        "css": ["*.css"],
        "js": ["*.js"]
      }
    ],
    "background": {
      "service_worker": "./background.js"
    },
    "options_page": "options.html",
    "open_in_tab": true,
    "permissions": [
      "unlimitedStorage",
      "downloads",
      "storage",
      "cookies",
      "activeTab",
      "scripting",
      "tabs",
      "file:///*/*"
  ] 
  ,
    "host_permissions": [ "https://*/*" ]
  }
}
