        "webRequest",
        "webRequestBlocking",
        "http://gtr.rcuk.ac.uk/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://gtr.rcuk.ac.uk/*"
            ],
            "js": [
                "/bower_components/lodash/dist/lodash.min.js",
                "/bower_components/d3/d3.min.js",
                "/scripts/api.js",
                "/scripts/app.js"
            ],
            "css": [
                "/styles/graphs.css"
            ]
        },
        {
            "matches": [
                "http://gtr.rcuk.ac.uk/person/*"
            ],
            "js": [
                "/scripts/person.graph.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "/images/toggle.png"
    ]
