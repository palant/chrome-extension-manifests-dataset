      ],
      "js": [
        "js/jquery/jquery.js",
        "js/jquery.hotkeys.js",
        "src/inject/inject.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "background" : {
    "persistent": false,
    "page": "src/background/background.html"
  },
  "browser_action": {
      "default_title": "xKey - Shortcut keys",
      "default_icon": {
          "19": "icons/icon19.png",
          "48": "icons/icon48.png"
      },
      "default_popup": "src/popup/popup.html"
