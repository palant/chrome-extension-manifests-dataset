        "js/commonFiles/sockjs.min.js",
        "js/commonFiles/stomp.min.js",
        "js/commonFiles/constants.js",
        "js/commonFiles/utils.js",
        "js/sajc/sajc-loader.js",
        "js/snjc/snjc-loader.js",
        "js/sajc/sajc.js",
        "js/snjc/snjc.js"
      ],
      "all_frames": true
