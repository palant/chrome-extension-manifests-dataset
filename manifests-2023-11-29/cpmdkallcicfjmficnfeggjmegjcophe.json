  "version": "0.2.0",
  "name": "MergEase",
  "description": "Boosts your code reviews by improving the GitHub pull request interface.",
  "background": {
    "service_worker": "serviceworker.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
