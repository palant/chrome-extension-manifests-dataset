      "resources": ["normal-128.png"],
      "matches": ["https://github.com/*"]
    },
    {
      "resources": ["swap-horizontal.svg"],
      "matches": ["https://github.com/*"]
    },
    {
      "resources": ["lock-outline.svg"],
      "matches": ["https://github.com/*"]
    },
    {
      "resources": ["alert-circle-outline.svg"],
      "matches": ["https://github.com/*"]
    }
  ],
  "permissions": ["storage"]
