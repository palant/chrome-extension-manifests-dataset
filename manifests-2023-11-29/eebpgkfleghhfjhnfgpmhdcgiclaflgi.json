        {
            "matches": [
                "https://*.github.com/*",
                "https://github.com/*"
            ],
            "run_at": "document_start",
            "js": [
                "scripts/main.min.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "scripts/content.min.js"
    ]
}