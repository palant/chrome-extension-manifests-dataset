  "action": {
    "default_name": "Urdu Reader",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    }
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "options_page": "options.html",
  "web_accessible_resources": [{
    "resources": ["*.woff2", "*.ttf"],
    "matches": ["<all_urls>"]
  }]
}
