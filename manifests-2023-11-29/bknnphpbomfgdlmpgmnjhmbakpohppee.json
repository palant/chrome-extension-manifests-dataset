  "manifest_version": 3,
  "name": "Tahajji - Urdu Reader",
  "version": "1.2",
  "description": "Read and Write Urdu in beautiful fonts using this open source extension.",
  "permissions": [ "storage", "scripting" ],
  "host_permissions": [ "<all_urls>" ],
  "background" : {
