      "css": [
        "content/content.css"
      ],
      "run_at": "document_end",
      "matches": [
        "<all_urls>"
      ],
      "all_frames": false
    },
    {
      "js": [
        "content/content-cloud.js"
      ],
      "run_at": "document_start",
      "matches": [
        "https://cloud.google.com/text-to-speech/*"
      ],
      "all_frames": false
    },
    {
      "run_at": "document_start",
      "matches": [
        "https://chat.openai.com/chat"
      ],
      "js": [
        "content/content-gpt.js"
      ],
      "css": [
        "styles/banner-gpt.css"
      ],
      "all_frames": true
    }
  ],
  "options_page": "options/options.html",
  "commands": {
    "forward": {
      "description": "Next",
      "suggested_key": {
