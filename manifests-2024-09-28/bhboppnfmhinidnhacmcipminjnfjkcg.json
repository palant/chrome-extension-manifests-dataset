{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "esa.io Auto Linkify",
  "description": "Make link Markdown format with post title.",
  "author": "Tomoyuki Kashiro",
  "version": "1.0.2",
  "background": {
    "scripts": ["src/js/background.js"],
    "persistent": false
  },
  "icons": {
    "16": "img/16.png",
    "32": "img/32.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "page_action":{
    "default_icon": {
      "16": "img/16.png",
      "32": "img/32.png",
      "48": "img/48.png",
      "128": "img/128.png"
    },
    "default_title":"esa.io Auto Linkify",
    "default_popup":"popup.html"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["https://*.esa.io/posts/*"],
      "js": ["dist/contents.js"]
    }
  ],
  "permissions":["tabs", "declarativeContent", "activeTab", "storage"]
}
