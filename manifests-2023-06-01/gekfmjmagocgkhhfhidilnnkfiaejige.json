{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
    
  "name": "B Marks!! - Google Bookmarks",
  "version": "0.5",
  "description": "Load and Display google bookmarks from your google account.",
  "icons": {
    "32": "bookmark-flag-icons.png",
