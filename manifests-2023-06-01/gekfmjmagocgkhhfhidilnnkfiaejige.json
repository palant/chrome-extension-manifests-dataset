  },

  "background":{
    "scripts":["background.js"]
  },

  "permissions": [
    "http://google.com/bookmarks",
    "http://*.google.com/", 
    "https://*.google.com/",
    "tabs",
    "activeTab"
  ],

  "browser_action": {
    "default_title": "BMarks",
    "default_icon": "bookmark-flag-icons.png",
    "default_popup": "popup.html"
  }
}