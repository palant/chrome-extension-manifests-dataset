{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "BitBucket Selfies",
  "description": "Selfies! For BitBucket! Hooray!",
  "version": "1.5",

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "permissions": [
    "https://bitbucket.org/*",
    "http://bitbucket.org/*"
  ],

  "content_scripts": [
    {
      "js": ["jquery.min.js", "selfie-base.js", "new-pr.js", "b64.js", "GIFEncoder.js", "LZWEncoder.js", "NeuQuant.js"],
      "matches": ["*://bitbucket.org/*/pull-request/new*"],
      "run_at": "document_end",
      "css": ["new-pr.css"]
    },
    {
      "js": ["jquery.min.js", "selfie-base.js", "diff.js", "b64.js", "GIFEncoder.js", "LZWEncoder.js", "NeuQuant.js"],
      "matches": ["*://bitbucket.org/*/pull-request/*/diff"],
      "run_at": "document_end",
      "css": ["diff.css"]
    }
  ],

  "web_accessible_resources": [
    "jquery-1.10.1.min.map"
  ]

}
