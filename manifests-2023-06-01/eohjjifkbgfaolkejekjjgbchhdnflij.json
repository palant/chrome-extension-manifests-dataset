{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "EtherDelta GDAX-like Theme",
  "description": "A GDAX-like theme for EtherDelta.",
  "version": "1.11",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://etherdelta.github.io/*",
        "https://etherdelta.github.io/*",
        "http://etherdelta.com/*",
        "https://etherdelta.com/*",
        "http://forkdelta.github.io/*",
        "https://forkdelta.github.io/*",
        "http://forkdelta.com/*",
        "https://forkdelta.com/*"
      ],
      "css": ["style.css"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    "fonts/btcglyph.eot",
    "fonts/btcglyph.svg",
    "fonts/btcglyph.ttf",
    "fonts/btcglyph.woff",
    "fonts/btcglyph.woff2",
    "fonts/opensans-bold-webfont.eot",
    "fonts/opensans-bold-webfont.svg",
    "fonts/opensans-bold-webfont.ttf",
    "fonts/opensans-bold-webfont.woff",
    "fonts/opensans-bold-webfont.woff2",
    "fonts/opensans-regular-webfont.eot",
    "fonts/opensans-regular-webfont.svg",
    "fonts/opensans-regular-webfont.ttf",
    "fonts/opensans-regular-webfont.woff",
    "fonts/opensans-regular-webfont.woff2",
    "style.css"
  ],
  "permissions": [
    "http://etherdelta.github.io/*",
    "https://etherdelta.github.io/*",
    "http://etherdelta.com/*",
    "https://etherdelta.com/*",
    "http://forkdelta.github.io/*",
    "https://forkdelta.github.io/*",
    "http://forkdelta.com/*",
    "https://forkdelta.com/*"
  ]
}
