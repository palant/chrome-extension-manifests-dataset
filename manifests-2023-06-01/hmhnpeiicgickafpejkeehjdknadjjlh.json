{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Edupage Addons",
  "version": "1.6.0",
  "manifest_version": 3,
  "description": "Enhance your Edupage experience",
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "*://*.edupage.org/*"
      ],
      "js": [
				"src/browser-polyfill.js",
        "src/inject/inject.js"
      ]
    }
  ],
  "permissions": ["storage"],
	"host_permissions": ["*://*.edupage.org/*"],
  "web_accessible_resources": [{
    "resources": ["src/injs/*"],
    "matches": ["*://*.edupage.org/*"]
	}],
  "action": {
    "default_popup": "src/popup/popup.html",
    "icons": {
      "16": "icons/icon16.png",
      "19": "icons/icon19.png",
      "32": "icons/icon32.png",
      "38": "icons/icon38.png",
      "48": "icons/icon48.png"
    }
  },
