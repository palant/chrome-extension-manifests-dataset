  },
  "permissions": [
    "https://hypothes.is/",
    "storage",
    "tabs",
    "unlimitedStorage",
    "https://www.mindmeister.com/oauth2",
    "https://www.mindmeister.com/services/rest/oauth2/token",
    "https://api.mendeley.com/*",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "https://api.github.com/*",
    "https://words.bighugelabs.com/api/*"
  ],
  "web_accessible_resources": [
    "images/*",
    "scripts/*",
    "scripts/adapters/*",
    "pages/*",
    "resources/*",
    "libs/*"
  ]
