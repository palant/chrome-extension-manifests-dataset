support_website: 'https://twitter.com/intent/tweet?screen_name=whochan&hashtags=LinkRedirector'
---

{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name"             : "LinkRedirector",
	"manifest_version" : 2,
	"version"          : "6.2.5",
	"default_locale"   : "en",
	"description"      : "__MSG_DESCRIPTION__",
	"homepage_url"     : "http://far.whochan.com/",
	"content_security_policy": "script-src 'self'; object-src 'self'",
	"icons"            :
	{
		"16"  : "icons/icon16.png",
		"32"  : "icons/icon32.png",
		"48"  : "icons/icon48.png",
		"64"  : "icons/icon64.png",
		"128" : "icons/icon112.png"
	},
	"background"       : {"page":"index.html"},
	"options_ui"        :
	{
		"chrome_style": false,
		"open_in_tab" : true,
		"page"        : "options.html"
	},
	"page_action"      :
	{
		"default_title": "LinkRedirector",
		"default_icon" : "buttons/enabled.png",
		"default_popup": "popup.html"
	},
	"permissions":
	[
		"http://*/*",
		"https://*/*",
		"tabs",
		"contextMenus",
		"webRequest",
		"webRequestBlocking"
	],
	"web_accessible_resources":
	[
		"cursors/*"
	],
	"content_scripts":
	[
		{
			"run_at"	: "document_start",
			"all_frames": true,
			"matches"	: ["<all_urls>"],
			"js"		:
			[
				"lib/WHO.extension.client.js",
				"includes/api.LinkFilter.js",
				"includes/usoCommon.js",
				"includes/usoLinkRedirector.js"
			]
		},
		{
			"run_at"	: "document_idle",
			"all_frames": true,
			"matches"	: ["<all_urls>"],
			"js"		: ["includes/WHO.activate.js"]
		}
	]}
