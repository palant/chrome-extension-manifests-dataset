      "background/imagehosts.js",
      "background/links.js",
      "background/processor.js",
      "background/background.js"
    ]
  },

  "browser_action": {
    "default_icon": "icons/imgding-32.png",
    "default_title": "imgding",
    "default_popup": "popup/imgding.html"
