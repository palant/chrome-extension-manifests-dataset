{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "MyTors",
    "description": "This extension is essential in order to use the MyTors.com website. It also provides you a quick search tool.",
    "version": "1.1.3",
    "icons": {
        "16": "img/icon.iconset/icon_16x16@2x.png",
        "48": "img/icon.iconset/icon_48x48@2x.png",
        "128": "img/icon.iconset/icon_128x128@2x.png",
        "512": "img/icon.iconset/icon_512x512@2x.png"
    },
    "background": {
        "presistent": false,
        "scripts": [
            "js/bg.js"
        ]
    },
    "permissions": [
        "*://*.torrentz.eu/",
        "*://*.torrentz.me/",
        "*://*.torrentz.ch/",
        "*://*.torrentz.in/",
        "*://*.bestdownload.eu/"
    ],
    "browser_action": {
        "default_icon": "img/icon.iconset/icon_16x16@2x.png",
        "default_title": "Search and download torrents using MyTors.com",
        "default_popup": "html/popup.html"
    },
    "content_scripts": [
        {
            "matches": ["*://*.mytors.com/*"],
            "js": ["js/lib/jquery.min.js","js/config.js","js/api.js"],
            "run_at": "document_start"
        }
    ]
}