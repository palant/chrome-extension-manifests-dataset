    "rule_resources": [
      {
        "id": "bilibili-video",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },

  "host_permissions": [
    "<all_urls>"
  ],
 "homepage_url": "https://www.pictureknow.com/",
