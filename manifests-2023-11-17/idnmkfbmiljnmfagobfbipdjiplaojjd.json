{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "HAL Formatter",
  "version": "0.1.2",
  "manifest_version": 2,
  "description": "Makes HAL easy to read. Open source.",
  "homepage_url": "https://github.com/osvaldopina/hal-chrome-extension",
  "minimum_chrome_version": "21",
  "short_name": "HAL Formatter",
  "icons": {
    "16": "src/assets/hal_16.png",
    "32": "src/assets/hal_32.png",
    "64": "src/assets/hal_64.png"
  },
  "web_accessible_resources": [
    "src/assets/Roboto-Regular-webfont.woff"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "polyfills-es5.js",
        "runtime-es5.js",
        "main-es5.js"
      ],
      "css": [
        "src/assets/indigo-pink.css",
        "src/assets/font-roboto.css"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "*://*/*",
    "<all_urls>",
    "storage"
  ]
}
