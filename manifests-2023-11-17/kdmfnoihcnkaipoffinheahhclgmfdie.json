---

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Greenlight",
  "version": "2.0.0",
  "manifest_version": 3,
  "description": "Greenlight makes it easy to give feedback on any website. Get rid of vague screenshots and infinite feedback loop!",
  "icons": {
    "128": "logo128.png"
  },
  "devtools_page": "devtools.html",
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "content_security_policy": {
    "script-src": "self",
    "object-src": "self"
  },
  "action": {
    "default_icon": "logo128.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens Greenlight extension"
    },
    "leave-comment": {
      "suggested_key": {
        "default": "Alt+C",
        "mac": "Alt+C"
      },
      "description": "Leave a spot comment"
    },
    "open-drawer": {
      "suggested_key": {
        "default": "Alt+O",
        "mac": "Alt+O"
      },
      "description": "Open comments drawer"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "cross.svg",
        "cursor.svg"
      ],
      "matches": ["http://*/*", "https://*/*"],
      "extension_ids": []
    }
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "cookies",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://staging.green-light.dev/*",
      "*://app.green-light.dev/*"
    ]
  },
  "key": ""
}
