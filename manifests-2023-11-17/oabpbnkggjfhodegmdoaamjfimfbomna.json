{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Chess Pro",
  "version": "2.1.1",
  "icons": { "128": "./assets/images/icon.png" },
  "content_scripts": [
    {
      "matches": [
        "http://*.lichess.org/*",
        "https://*.lichess.org/*"
      ],
      "js": ["./scripts/build.js"],
      "run_at": "document_start"
    }
